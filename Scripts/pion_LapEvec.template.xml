<?xml version="1.0"?>
<grid>
  <parameters>
    <trajCounter>
      <start>{cfg}</start>
      <end>{end}</end>
      <step>{step}</step>
    </trajCounter>
    <genetic>
      <popSize>20</popSize>
      <maxGen>1000</maxGen>
      <maxCstGen>100</maxCstGen>
      <mutationRate>0.1</mutationRate>
    </genetic>
    <runId>pion_{cfg}</runId>
    <graphFile></graphFile>
    <parallelWriteMaxRetry>-1</parallelWriteMaxRetry>
  </parameters>
  <modules>
    <module>
      <id>
        <name>LapEvec_gauge</name>
        <type>MIO::LoadNersc</type>
      </id>
      <options>
        <file>/tessfs1/work/dp008/dp008/shared/dwf_2+1f/C1/ckpoint_lat</file>
      </options>
    </module>
    <module>
      <id>
        <name>LapEvec</name>
        <type>MDistil::LapEvec</type>
      </id>
      <options>
        <gauge></gauge>
        <Stout>
          <steps>3</steps>
          <rho>0.2</rho>
        </Stout>
        <Cheby>
          <PolyOrder>11</PolyOrder>
          <alpha>0.55</alpha>
          <beta>12.5</beta>
        </Cheby>
        <Lanczos>
          <Nvec>100</Nvec>
          <Nk>120</Nk>
          <Np>20</Np>
          <MaxIt>1000</MaxIt>
          <resid>1e-8</resid>
          <IRLLog>0</IRLLog>
        </Lanczos>
      </options>
    </module>
  </modules>
</grid>
