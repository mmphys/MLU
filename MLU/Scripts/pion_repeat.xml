<?xml version="1.0"?>
<grid>
  <parameters>
    <trajCounter>
      <start>3000</start>
      <end>3001</end>
      <step>3040</step>
    </trajCounter>
    <genetic>
      <popSize>20</popSize>
      <maxGen>1000</maxGen>
      <maxCstGen>100</maxCstGen>
      <mutationRate>0.1</mutationRate>
    </genetic>
    <runId>pion_3000</runId>
    <graphFile></graphFile>
    <scheduleFile></scheduleFile>
    <saveSchedule>false</saveSchedule>
    <parallelWriteMaxRetry>-1</parallelWriteMaxRetry>
  </parameters>
  <modules>
    <module>
      <id>
        <name>LapEvec_gauge</name>
        <type>MIO::LoadNersc</type>
      </id>
      <options>
        <file>/tessfs1/work/dp008/dp008/shared/dwf_2+1f/C1/ckpoint_lat</file>
      </options>
    </module>
    <module>
      <id>
        <name>DWF</name>
        <type>MAction::DWF</type>
      </id>
      <options>
        <gauge>LapEvec_gauge</gauge>
        <Ls>16</Ls>
        <mass>0.005</mass>
        <M5>1.8</M5>
        <boundary>1 1 1 -1</boundary>
        <twist>0. 0. 0. 0.</twist>
      </options>
    </module>
    <module>
      <id>
        <name>CG</name>
        <type>MSolver::RBPrecCG</type>
      </id>
      <options>
        <action>DWF</action>
        <maxIteration>10000</maxIteration>
        <residual>1e-7</residual>
        <eigenPack></eigenPack>
      </options>
    </module>
    <module>
      <id>
        <name>LapEvec</name>
        <type>MDistil::LapEvec</type>
      </id>
      <options>
        <gauge></gauge>
        <Stout>
          <steps>3</steps>
          <rho>0.2</rho>
        </Stout>
        <Cheby>
          <PolyOrder>11</PolyOrder>
          <alpha>0.55</alpha>
          <beta>12.5</beta>
        </Cheby>
        <Lanczos>
          <Nvec>100</Nvec>
          <Nk>120</Nk>
          <Np>20</Np>
          <MaxIt>1000</MaxIt>
          <resid>1e-8</resid>
          <IRLLog>0</IRLLog>
        </Lanczos>
      </options>
    </module>
    <module>
      <id>
        <name>Peramb_noise</name>
        <type>MDistil::Noises</type>
      </id>
      <options>
        <nnoise>1</nnoise>
        <nvec>100</nvec>
        <UniqueIdentifier></UniqueIdentifier>
        <TI></TI>
        <LI></LI>
      </options>
    </module>
    <module>
      <id>
        <name>Peramb</name>
        <type>MDistil::Perambulator</type>
      </id>
      <options>
        <lapevec>LapEvec</lapevec>
        <solver>CG</solver>
        <noise></noise>
        <PerambFileName>PerambL_100_tsrc0</PerambFileName>
        <nvec>100</nvec>
        <Distil>
          <nnoise>1</nnoise>
          <tsrc>0</tsrc>
          <TI></TI>
          <LI></LI>
          <SI></SI>
        </Distil>
      </options>
    </module>
    <module>
      <id>
        <name>DistilVecs</name>
        <type>MDistil::DistilVectors</type>
      </id>
      <options>
        <noise>Peramb_noise</noise>
        <perambulator>Peramb</perambulator>
        <lapevec>LapEvec</lapevec>
        <source></source>
        <sink></sink>
        <tsrc>0</tsrc>
        <nvec></nvec>
        <TI></TI>
      </options>
    </module>
    <module>
      <id>
        <name>DistilMesonFieldPhi</name>
        <type>MContraction::A2AMesonField</type>
      </id>
      <options>
        <cacheBlock>2</cacheBlock>
        <block>4</block>
        <left>DistilVecs_phi</left>
        <right>DistilVecs_phi</right>
        <output>MesonFieldPhiL</output>
        <gammas>all</gammas>
        <mom>
          <elem>0 0 0</elem>
        </mom>
      </options>
    </module>
    <module>
      <id>
        <name>DistilMesonFieldRho</name>
        <type>MContraction::A2AMesonField</type>
      </id>
      <options>
        <cacheBlock>2</cacheBlock>
        <block>4</block>
        <left>DistilVecs_rho</left>
        <right>DistilVecs_rho</right>
        <output>MesonFieldRhoL</output>
        <gammas>all</gammas>
        <mom>
          <elem>0 0 0</elem>
        </mom>
      </options>
    </module>
  </modules>
</grid>
